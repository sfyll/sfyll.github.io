{
  "name": "sfyl-site",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build && cd scripts/ && node updateMetaTags.js",
    "lint": "vue-cli-service lint",
    "dev": "vue-cli-service serve",
    "deploy": "gh-pages -d dist",
    "deployjob": "gh-pages -d dist -b gh-pages -r https://${GITHUB_TOKEN}@github.com/SFYLL/sfyll.github.io.git",
    "serve-dist": "http-server dist"
  },
  "dependencies": {
    "@vue/compiler-dom": "^3.2.40",
    "a17t": "^0.1.4",
    "axios": "^0.21.2",
    "buttercms": "^1.2.9",
    "core-js": "^3.6.5",
    "date-fns": "^2.28.0",
    "front-matter": "^4.0.2",
    "fs-extra": "^11.1.1",
    "gh-pages": "^6.0.0",
    "jsdom": "^22.1.0",
    "katex": "^0.16.22",
    "marked": "^9.0.3",
    "roboto-mono-webfont": "^2.0.986",
    "simple-icons": "^5.0.0",
    "vue": "^3.2.36",
    "vue-gtag": "^2.0.1",
    "vue-meta": "^3.0.0-alpha.10",
    "vue-router": "^4.0.13",
    "vue-tooltip": "^0.1.0",
    "vue-unicons": "^3.3.1",
    "vuex": "^4.0.2",
    "vuex-persist": "^2.2.0",
    "vuex-persistedstate": "^4.1.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.22.7",
    "@babel/plugin-transform-private-methods": "^7.22.5",
    "@prerenderer/webpack-plugin": "^5.3.6",
    "@vue/cli-plugin-babel": "~5.0.8",
    "@vue/cli-plugin-eslint": "~5.0.8",
    "@vue/cli-plugin-router": "~5.0.8",
    "@vue/cli-plugin-vuex": "~5.0.8",
    "@vue/cli-service": "~5.0.8",
    "copy-webpack-plugin": "^11.0.0",
    "eslint-plugin-vue": "^9.15.1",
    "http-server": "^14.1.1",
    "prerender-spa-plugin-next": "^4.2.3",
    "vue-cli-plugin-tailwind": "~1.5.0",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parser": "vue-eslint-parser",
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {
      "vue/multi-word-component-names": "off",
      "vue/no-reserved-component-names": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
